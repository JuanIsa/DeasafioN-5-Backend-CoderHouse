<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Producto</th>
            <th scope="col">Precio</th>
            <th scope="col">Foto</th>
        </tr>
    </thead>
    <tbody>
        {{#each dataFile}}
            {{> Tablerow item=this}}
        {{/each}}
    </tbody>
</table>
{{#if renderConditional}}
<h3>No hay productos en la lista.</h3>
{{/if}}