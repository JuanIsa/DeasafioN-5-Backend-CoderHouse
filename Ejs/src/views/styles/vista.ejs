<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Producto</th>
            <th scope="col">Precio</th>
            <th scope="col">Foto</th>
        </tr>
    </thead>
    <tbody>
        <% if(renderConditional){ %>
            <%datafile.forEach(function(producto) {%>
                <tr>
                    <th scope="col">
                        <%=producto.id%>
                    </th>
                    <th scope="col">
                        <%=producto.title%>
                    </th>
                    <th scope="col">
                        <%=producto.price%>
                    </th>
                    <th scope="col">
                        <img class="fotos" src=<%=producto.thumbnail%>>
                    </th>
                </tr>
                <%});%>
                    <% } else{ %>
                        <h1>No hay productos en la lista.</h1>
                        <% } %>

    </tbody>
</table>